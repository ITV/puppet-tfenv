{
  "name": "sergk-tfenv",
  "version": "0.2.1",
  "author": "Sergiy Kulanov",
  "summary": "Install and configure tfenv tool which manages terraform",
  "license": "Apache-2.0",
  "source": "https://github.com/SergK/puppet-tfenv",
  "project_page": "https://github.com/SergK/puppet-tfenv",
  "issues_url": "https://github.com/SergK/puppet-tfenv/issues",
